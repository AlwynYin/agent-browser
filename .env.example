# Agent Browser Configuration
# Copy this file to .env and fill in your values

# Database
MONGO_URL=mongodb://localhost:27017/agent-browser

# AI Services (required)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Browser-Use Cloud API (required for browser agent)
BROWSER_USE_API_KEY=bu_your-browser-use-api-key-here

# Python Execution API (external service)
PYTHON_EXECUTION_API_URL=http://localhost:8000
PYTHON_EXECUTION_API_KEY=optional-api-key

# Server Configuration
PORT=3001
NODE_ENV=development
CORS_ORIGINS=http://localhost:3000